{
  "name": "chartgpt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node build/bin/www.mjs",
    "dev": "nodemon ./src/bin/www.mts",
    "debug": "cross-env DEBUG=* nodemon ./src/bin/www.mts",
    "dev:front": "npm run --prefix ./frontend dev",
    "build:front": "npm run --prefix ./frontend build",
    "prd": "pm2 start build/bin/www.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-ts": "tsc",
    "copy-static": "node src/copyStatic.mjs",
    "build": "npm run build-ts && npm run copy-static",
    "tsc": "tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "prettier:check": "prettier '**/*.{ts,tsx,mts,mjs,less,js}' --check",
    "prettier:write": "prettier '**/*.{ts,tsx,mts,mjs,less,js}' --write"
  },
  "dependencies": {
    "@koa/router": "^12.0.0",
    "@types/debug": "^4.1.7",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-static": "^4.0.2",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.14.0",
    "@types/qs": "^6.9.7",
    "abortcontroller-polyfill": "^1.7.5",
    "chatgpt": "^5.0.6",
    "dayjs": "^1.11.7",
    "debug": "^4.1.1",
    "https-proxy-agent": "^5.0.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-etag": "^3.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "koa2-history-api-fallback": "^0.1.0",
    "lodash-es": "^4.17.21",
    "node-fetch": "^3.3.0",
    "pug": "^2.0.3",
    "qs": "^6.11.1"
  },
  "lint-staged": {
    "*.{ts,tsx,mts,mjs,less,js}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/shelljs": "^0.8.11",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "nodemon": "^1.19.1",
    "prettier": "^2.8.7",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "openai",
    "chatgpt",
    "chat",
    "gpt",
    "gpt-3",
    "gpt3",
    "gpt4",
    "ai",
    "ai-assistant"
  ]
}
